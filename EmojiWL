local StarId = {
    [3] = true,
}

local CrownId = {}

local DiamondId = {
    [2] = true,
}

function premium()
    for _,v in pairs(game:GetService('Players'):GetChildren()) do
        if StarId[v.UserId] then
            if v.Character then
                if v.Character.Parent.Name == 'Players' then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[:star:]'..v.DisplayName)
                end
            end
        elseif CrownId[v.UserId] then
            if v.Character then
                if v.Character.Parent.Name == 'Players' then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[:crown:]'..v.DisplayName)
                end
            end
        elseif DiamondId[v.UserId] then
            if v.Character then
                if v.Character.Parent.Name == 'Players' then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[:gem:]'..v.DisplayName)
                end
            end
        elseif v.Character then
            if v.Character.Parent.Name == 'Players' then
                if not v.Character.UpperTorso:FindFirstChild('LeftCollarAttachment') then
                    v.Character:FindFirstChildWhichIsA('Humanoid').DisplayName = ('[:slight_smile:]'..v.DisplayName)
                end
            end
        end
    end
end
local success,err = pcall(premium)
